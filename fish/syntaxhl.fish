# Fish syntax highlighting
set -g fish_color_normal white
set -g fish_color_command blue --bold
set -g fish_color_keyword blue
set -g fish_color_quote yellow --italics
set -g fish_color_redirection red
set -g fish_color_end white
set -g fish_color_error purple
set -g fish_color_param white
set -g fish_color_comment white --dim --italics
set -g fish_color_selection white  --bold  --background=brblack
set -g fish_color_operator white
set -g fish_color_escape de935f
set -g fish_color_autosuggestion 555
set -g fish_color_cancel 555
set -g fish_color_history_current --bold
set -g fish_color_match --background=brblue
set -g fish_color_valid_path white
